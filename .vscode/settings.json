{
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "html",
        "vue",
        "markdown"
    ],
    "editor.codeActionsOnSave": {
        
        "source.fixAll.eslint": "explicit"
    },
    "editor.tabSize": 4,
    "editor.fontSize": 11,
}